{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    },
    "lib": [
      "dom",
      "es6",
      "scripthost",
      "es2015.iterable"
    ],
    "target": "es6"
}
